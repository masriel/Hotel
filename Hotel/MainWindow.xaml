<Window x:Class="Hotel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Height="745" Width="1045"
        x:Name="MainForm" WindowStartupLocation="CenterScreen"
        WindowStyle="None" Title="АИС Гостиница"
        AllowsTransparency="True" Background="Transparent"
        ResizeMode="NoResize" FontSize="18"
        FontFamily="{DynamicResource MaterialDesignFont}">
    <Grid>
        <Frame x:Name="MainWindowFrame" Background="Transparent"
               NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
